{
  "name": "AKA Prefixer",
  "description": "Prefixes aka.ms to the keyword",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "src/popup.html"
  },
  "options_ui": {
    "page": "src/options.html"
  },
  "icons": { "16": "src/images/icon16.png", "32": "src/images/icon32.png", "48": "src/images/icon48.png", "128": "src/images/icon128.png" },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      }
    }
  }
}
